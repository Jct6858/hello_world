void setup()
{
  size(550, 180);
  smooth();
  strokeWeight(12);
  rectMode(CENTER);
  rect(40, 25, 70, 70);
  strokeJoin(BEVEL);
  rectMode(CENTER);
  rect(140, 25, 70, 70);
  strokeCap(SQUARE);
  line(270, 25, 340, 95);
  strokeCap(ROUND);
  line(350, 25, 420, 95);
  ellipseMode(CENTER);
  ellipse(480, 50, 35, 35);
}
