void setup()
{
  size(480, 120);
smooth();

beginShape();
fill(0, 0, 255, 160);
vertex(20, 100);
vertex(120, 110);
vertex(220, 100);
vertex(160, 60);
vertex(120, 70);
vertex(80, 60);
vertex(20,100);
endShape();
fill(204, 226, 225);

beginShape();
vertex(120, 60);
vertex(150, 50);
vertex(120, 35);
vertex(90, 50);
vertex(120, 60);
endShape();
}
